{
  "version": 2,
  "builds": [
    {
      "src": "generate_site_vercel.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "src": "output/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/output/$1"
    }
  ]
} 